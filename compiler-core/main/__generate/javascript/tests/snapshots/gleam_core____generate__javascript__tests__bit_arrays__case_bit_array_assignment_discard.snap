export function go(x) {
  if (x.bitSize === 8) {
    let n = x.byteAt(0);
    return n;
  } else if (x.bitSize >= 8 && (x.bitSize - 8) % 8 === 0) {
    let n = x.byteAt(0);
    return n;
  } else {
    return 1;
  }
}
